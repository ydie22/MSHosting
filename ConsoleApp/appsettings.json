{
  "ConnectionStrings": {
    "RabbitMQ": "amqp://guest:guest@localhost//?heartbeat=600"
  },
  "Kestrel": {
    "Endpoints": {
      "InsecureGrpc": {
        "Url": "http://localhost:55555",
        "Protocols": "Http2"
      },
      "InsecureHttp11": {
        "Url": "http://localhost:55557",
        "Protocols": "Http1"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Formatting.Compact" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ]
  } }
